root = true

[*]
charset = utf-8
max_line_length = 80
indent_size = 2
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.py]
indent_style = space
indent_size = 4

[*.ipynb]
indent_style = space
indent_size = 4
trim_trailing_whitespace = false
insert_final_newline = false
